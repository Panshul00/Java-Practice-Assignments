package com.repo;

import java.util.HashMap;
import java.util.Map;

public class Repo {
	static Map<User, String> users;
	static {
		users = new HashMap<User, String>();
		users.put(new User("Admin","admin123","Manager"), "Admin");
		users.put(new User("HR","hr","Human Resource"), "HR");
	}

	public static Map<User, String> getUsers() {
		return users;
	}

}
